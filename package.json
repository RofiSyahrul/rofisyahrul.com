  "scripts": {
    "astro:dev": "astro dev",
    "astro:build": "astro build",
    "astro:start": "astro preview",
    "build": "pnpm generate-manifest && pnpm build:css:prod && pnpm build:remix && pnpm generate-sw",
    "build:local": "pnpm clean && pnpm generate-manifest && pnpm build:css:prod && BUILD_ENV=local pnpm build:remix && pnpm generate-sw --debug",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "build:css": "postcss styles/**/*.css --base styles --dir app/styles",
    "build:css:prod": "pnpm build:css --env production",
    "clean": "rm -rf api build public/build",
    "css:watch": "pnpm build:css --w",
    "data-gen": "node -r esbuild-register scripts/data-gen && eslint app/data --ext .ts --color --fix",
    "dev": "pnpm clean && pnpm generate-manifest && concurrently -k -p \"[{name} @ {time}]\" -n \"CSS,Remix\" -c \"cyan.bold,green.bold\" \"pnpm css:watch\" \"pnpm dev:remix\"",
    "dev:remix": "cross-env NODE_ENV=development PORT=8765 remix dev",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx . --color --cache",
    "eslint:fix": "pnpm eslint --fix",
    "generate-manifest": "node -r esbuild-register scripts/generate-manifest.ts",
    "generate-sw": "node -r esbuild-register -r dotenv/config ./scripts/generate-sw.ts",
    "get-spotify-refresh-token": "node -r esbuild-register -r dotenv/config ./scripts/spotify/get-refresh-token",
    "lint": "pnpm eslint && pnpm stylelint",
    "lint:fix": "pnpm eslint:fix && pnpm stylelint:fix",
    "postinstall": "remix setup node",
    "start:local": "PORT=9706 node -r dotenv/config node_modules/.bin/remix-serve build",
    "stylelint": "stylelint styles/**/*.css --color",
    "stylelint:fix": "pnpm stylelint --fix"
  },
  "sideEffects": false,
  "type": "module",
  "version": "0.1.0"
}
