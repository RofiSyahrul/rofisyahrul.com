  "scripts": {
    "build": "yarn build:css:prod && yarn build:remix",
    "build:local": "yarn clean && yarn generate-manifest && yarn build:css:prod && BUILD_ENV=local yarn build:remix",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "build:css": "postcss styles/**/*.css --base styles --dir app/styles",
    "build:css:prod": "yarn build:css --env production",
    "clean": "rm -rf api build public/build",
    "css:watch": "yarn build:css --w",
    "data-gen": "node -r esbuild-register scripts/data-gen",
    "dev": "concurrently -k -p \"[{name} @ {time}]\" -n \"CSS,Remix\" -c \"cyan.bold,green.bold\" \"yarn css:watch\" \"yarn dev:remix\"",
    "dev:remix": "cross-env NODE_ENV=development PORT=8765 remix dev",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx . --color --cache",
    "eslint:fix": "yarn eslint --fix",
    "generate-manifest": "node -r esbuild-register scripts/generate-manifest.ts",
    "generate-sw": "node -r esbuild-register -r dotenv/config ./scripts/generate-sw.ts",
    "lint": "yarn eslint && yarn stylelint",
    "lint:fix": "yarn eslint:fix && yarn stylelint:fix",
    "postbuild": "yarn generate-sw",
    "postbuild:local": "yarn generate-sw --debug",
    "postdata-gen": "eslint app/data --ext .ts --color --fix",
    "postinstall": "remix setup node",
    "prebuild": "yarn generate-manifest",
    "predev": "yarn clean && yarn generate-manifest",
    "start:local": "PORT=9706 node -r dotenv/config node_modules/.bin/remix-serve build",
    "stylelint": "stylelint styles/**/*.css --color",
    "stylelint:fix": "yarn stylelint --fix"
  },
  "sideEffects": false,
  "version": "0.1.0"
}
